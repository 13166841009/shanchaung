<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:tools="http://schemas.android.com/tools"
    android:orientation="vertical"
    tools:context="ruanjianbei.wifi.com.Phone_P_3G.upload.uploadActivity">
    <view.TitleBarView
        android:id="@+id/title_bar"
        android:layout_height="wrap_content"
        android:layout_width="match_parent"/>

    <EditText
        android:id="@+id/et_filepath"
        android:layout_width="match_parent"
        android:layout_height="50dp"
        android:padding="5dp"
        android:layout_centerInParent="true"
        android:layout_below="@+id/title_bar"
        android:background="@drawable/login_et_bg"
        android:layout_marginTop="20dp"
        android:layout_margin="2dp"
        android:hint="请输入传输对象用户名" />

    <ListView
        android:id="@+id/list_choosed"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/et_filepath"
        android:layout_above="@+id/mi_btn">
    </ListView>


    <ruanjianbei.wifi.com.Phone_P_3G.upload.util.MiSportButton
        android:id="@+id/mi_btn"
        android:layout_width="match_parent"
        android:layout_height="100dp"
        android:layout_above="@+id/pro"
        android:layout_marginBottom="0dp">

    </ruanjianbei.wifi.com.Phone_P_3G.upload.util.MiSportButton>


    <ruanjianbei.wifi.com.Phone_P_3G.upload.util.RoundProgressBarWidthNumber
        android:id="@+id/pro"
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:layout_alignParentBottom="true"
        android:layout_centerHorizontal="true"
        android:layout_marginBottom="14dp"
        android:background="#fff"
        android:padding="10dp"
        android:progress="0"
        android:indeterminate="false" />

</RelativeLayout>
